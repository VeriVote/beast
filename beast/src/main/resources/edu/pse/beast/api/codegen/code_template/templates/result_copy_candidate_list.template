    RESULT_TYPE RESULT_VAR;
    RESULT_VAR.AMT_MEMBER == NONDET_UINT();
    ASSUME(RESULT_VAR.AMT_MEMBER == CURRENT_AMT_CAND);
    for (int i = 0; i < CURRENT_AMT_CAND; ++i) {
        RESULT_VAR.LIST_MEMBER[i] = NONDET_UINT();
        ASSUME(RESULT_VAR.LIST_MEMBER[i] == RESULT_ARR[i]);
    }
    for (int i = CURRENT_AMT_CAND; i < MAX_AMT_CANDIDATES; ++i) {
        RESULT_VAR.LIST_MEMBER[i] = NONDET_UINT();
        ASSUME(RESULT_VAR.LIST_MEMBER[i] == INVALID_RESULT);
    }
